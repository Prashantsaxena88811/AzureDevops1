                                        </aura:iteration>
                                    </div>
                                </aura:if>
                            </div>
                        </aura:iteration>

                    </div>
                </div>
            </div>

        </div>

    </aura:if>

    <aura:if isTrue="{!v.showMobileView}">
        <div class="productList">
            <div class="order-summary">
                <div class="order-details">
                    <div class="order-info">
                        <div class="order-date">
                            <label class="date">{!$Label.c.EC_Order_OrderSection_DateLabel}</label>
                            <!-- <span class="lbl-value">{!v.order.orderDate}</span> -->
