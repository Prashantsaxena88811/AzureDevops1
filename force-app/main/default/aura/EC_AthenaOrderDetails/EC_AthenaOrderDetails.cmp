                            <lightning:button onclick="{!c.showProducts}"
                                label="{!$Label.c.EC_Order_OrderSection_ViewMoreButton}" value="{!v.order}" />
                        </div>
                    </div>
                </div>

                <div class="placed-by">
                    <div><label class="lbl-name">{!$Label.c.EC_Order_OrderSection_POLabel}</label><label
                            class="lbl-value"> {!v.order.orderCustomerPO}</label></div>
                </div>

                <aura:if isTrue="{!v.isOnline}">
                    <div class="btn-container">
                        <lightning:button class="ec_btn_primary" label="{!$Label.c.EC_Order_OrderSection_ReorderButton}"
                            title="{!$Label.c.EC_Order_OrderSection_ReorderButton}" onclick="{!c.addItemToCart}"
                            value="{!v.order}" />
                    </div>
                </aura:if>

                <div class="list slds-hide" id="{!v.order.orderNum}">
                    <div class="shipping-address">
                        <div class="address-heading">
                            <span>
                                <i class="material-icons icon-color">room</i>
                            </span>
                            <span class="left-spacer"> {!$Label.c.EC_OrderDetails_ShippingAddress}: </span>
                        </div>
                            
                        <div class="value">
                            <div class="shipToName">{!$Label.c.EC_OrderDetails_ShipToName}:
                                <span class="val">{!v.order.orderShipToName}/{!v.order.orderShipToNumber}</span>
                            </div>
                            <aura:unescapedHtml value="{!v.order.shipToAddress}" />
                        </div>
                    </div>
                    <!--<div class="billing-address">
                        <div class="address-heading">
                            <span>
                                <i class="material-icons icon-color">file_copy</i>
                            </span>
                            <span class="left-spacer"> {!$Label.c.EC_OrderDetails_BillingAddress}: </span>
                        </div>
                        <div class="value">
                            <aura:unescapedHtml value="{!v.order.billToAddress}" />

                        </div>
                    </div>-->
                    <aura:iteration items="{!v.order.orderLineItems}" var="orderDtl" indexVar="indx">

                        <div class="product-list">
                            <div class="product-details">
                                <div class="image">
                                    <aura:if isTrue="{!orderDtl.ProdImg}">
                                        <img src="{!orderDtl.ProdImg}" onerror="{!c.imageError}" />
                                        <aura:set attribute="else">
                                            <img src="{!$Resource.EC_Branding_Theme + '/images/product.png'}" />
                                        </aura:set>
                                    </aura:if>
                                </div>
                                <div class="product-details-info">
                                    <div class="product-list-info">
                                        <div class="product-name">
                                            <a href="{!orderDtl.PrdDetUrl}" target="_blank">{!orderDtl.orderLineMaterialName}</a>
                                        </div>
                                        <div class="prod-desc">
                                            <span>{!$Label.c.EC_OrderDetails_ProductCode}:</span> <span class="value">{!orderDtl.orderLineMaterialNum}</span>
                                            <span class="no-of-products">{!orderDtl.orderLineMaterialUoM}</span>
                                            <aura:if isTrue="{!v.viewPrice}">
                                                <div class="price">
                                                    <!--<label class="dollar">{!$Label.c.EC_Dollar}{!orderDtl.Dollars}</label>-->
                                                    <label class="cent">{!orderDtl.Cents}</label>
                                                </div>
                                            </aura:if>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="price-quantity-info">
                                <div>
                                    <label class="lbl-name">{!$Label.c.EC_OrderDetails_TotalQuantity}: </label>
                                    <label class="lbl-value"><ui:outPutNumber aura:id="myNumberFormat" format="0" value="{!orderDtl.orderLineQuantity}"/></label>
                                </div>
                                <!--<div>
                                    <label class="lbl-name">{!$Label.c.EC_OrderDetails_Price}: </label>
                                    <label class="lbl-value">${!orderDtl.UnitPrice} </label>
                                </div>-->
                                <aura:iteration items="{!v.orderLineStatuses}" var="lineStatus" indexVar="lineStatusInd">
                                    <aura:if isTrue="{!lineStatusInd == indx}">
                                        <div class="inline-status">
                                            <label>{!$Label.c.EC_Order_OrderSection_LineStatusLabel}: </label>
                                            <div class="{!'statusvalue ' + lineStatus}">{!lineStatus}</div>
                                        </div>
                                    </aura:if>
                                </aura:iteration>
                            </div>

                            <!--fourkites shipment tracking-->
                            <aura:if isTrue="{!orderDtl.deliveryExist == 'Y'}">
                                <aura:if isTrue="{!and(orderDtl.deliveries != null,orderDtl.deliveries.length != 0)}">
                                    <aura:if isTrue="{!orderDtl.deliveries[0].trackUrl != null}">
                                        <div class="tracking">
                                            <div class="track-shipment">
                                                <img src="{!$Resource.EC_Branding_Theme + '/images/icons/outline-local-shipping.svg'}" alt="icon"/>
                                                <lightning:button variant="base"
                                                                  label="{!$Label.c.EC_OrderDetails_TrackShipment}"
                                                                  title="{!$Label.c.EC_OrderDetails_TrackShipment}"
                                                                  onclick="{! c.openModel }" />
                                            </div>
                                            
                                            <!--Use aura:if tag to display/hide popup based on isModalOpen value-->  
                                            <aura:if isTrue="{!v.isModalOpen}">
                                                
                                                <!-- Modal/Popup Box starts here-->
                                                <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" 
                                                         class="slds-modal slds-fade-in-open slds-modal_medium">
                                                    <div class="slds-modal__container">
                                                        <!-- Modal/Popup Box Header Starts here-->
                                                        <header class="slds-modal__header">
                                                            <lightning:buttonIcon iconName="utility:close"
                                                                                  onclick="{! c.closeModel }"
                                                                                  alternativeText="close"
                                                                                  variant="bare-inverse"
                                                                                  class="slds-modal__close"/>
                                                        </header>
                                                        <!--Modal/Popup Box Body Starts here-->
                                                        <iframe src="{!orderDtl.deliveries[0].trackUrl}" width="100%" height="85%" frameBorder="0"/>
                                                    </div>
                                                </section>
                                                <div class="slds-backdrop slds-backdrop_open"></div>
                                            </aura:if>
                                        </div>
                                    </aura:if>
                                </aura:if>
                            </aura:if>

                            <aura:iteration items="{!orderDtl.ProdSDSURI}" var="sdsURI" indexVar="indx">
                                <div class="document-name">
                                    <span>SDS Document 1</span>
                                    <span class="doc-url">
                                        <i class="material-icons">
                                            <a href="{!sdsURI}" target="_blank">open_in_new</a>
                                        </i>
                                    </span>
                                </div>
                            </aura:iteration>
                        </div>
                    </aura:iteration>
                </div>

            </div>
        </div>
    </aura:if>
</aura:component>