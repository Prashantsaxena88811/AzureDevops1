                            <div class="sort-pagination">
                                <span class="pagination-wrapper">
                                    <span class="page-info">
                                        {!lessthan(v.productPagination.total,
                                        v.pageSize) ? v.productPagination.total :
                                        v.productPagination.per_page}&nbsp;{!$Label.c.EC_Order_Results_OutOf}&nbsp;{!v.productPagination.total}&nbsp;items
