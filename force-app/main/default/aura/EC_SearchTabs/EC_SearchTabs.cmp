                                                            <div class="{! pageNumber == v.ordersPagination.page ? 'page_item active' : pageNumber == '...' ? 'page_item disabled' :  'page_item'}"
                                                                 data-value="{!pageNumber}" onclick="{!c.changePage}">
                                                                {!pageNumber}
                                                            </div>
                                                        </aura:iteration>
                                                    </div>
                                                </div>
                                                <div class="slds-hide_medium">
                                                    <div class="pages">
                                                        <aura:iteration items="{!v.ordersPagination.mobPageOptions}"
                                                                        var="pageNumber">
                                                            <div class="{! pageNumber == v.ordersPagination.page ? 'page_item active' : pageNumber ? 'page_item disabled' :  'page_item'}"
                                                                 data-value="{!pageNumber}" onclick="{!c.changePage}">
                                                                {!pageNumber}
                                                            </div>
                                                        </aura:iteration>
                                                    </div>
                                                </div>
                                                
                                                <div class="{!v.ordersPagination.next_action ? 'prev_next disabled' : 'prev_next'}"
