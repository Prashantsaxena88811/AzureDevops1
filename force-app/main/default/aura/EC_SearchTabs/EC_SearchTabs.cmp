<aura:component controller="EC_GlobalSearchController"
    implements="flexipage:availableForAllPageTypes,forceCommunity:availableForAllPageTypes" access="global">
    <aura:attribute type="List" name="productsList" />
    <aura:attribute type="String" name="SearchTextField" />
    <aura:attribute type="String" name="SearchQuickList" />
    <aura:attribute name="emptyList" type="Boolean" default="false" />
    <aura:attribute name="emptyOrderList" type="Boolean" default="false" />
    <aura:attribute name="ordersCount" type="String" />
    <aura:attribute name="DocCount" type="String" />
    <aura:attribute name="SearchText" type="String" />
    <aura:attribute name="emptyDocumentsWrapper" type="Boolean" default="false" />
    <aura:attribute type="List" name="sdsDocsList" />
    <aura:attribute name="Orders" type="List" />
    <aura:attribute name="emptyQuickList" type="Boolean" default="false" />
    <aura:attribute name="quickListCount" type="String" />
    <aura:attribute type="EC_GlobalSearchController.prodBean[]" name="QuickList" />
    <aura:attribute name="searchExceptQuicklist" type="Boolean" />
    <aura:attribute name="pageSize" type="Integer" default="25" />

    <!-- Product Pagination attributes -->
    <aura:attribute type="List" name="quicklistMasterList" />
    <aura:attribute name="quicklistSortBy" type="String" access="PRIVATE" default="purchasedOn|DESC" />
    <aura:attribute name="quicklistPagination" type="Map" />
    <aura:attribute name="currentQuickListPage" type="Integer" default="1" />
    <!-- Product Pagination attributes ends -->
    <!-- Product Pagination attributes -->
    <aura:attribute type="List" name="documentsMasterList" />
    <aura:attribute name="documentsSortBy" type="String" access="PRIVATE" default="productPurchaseDate|DESC" />
    <aura:attribute name="documentsPagination" type="Map" />
    <aura:attribute name="currentDocumentPage" type="Integer" default="1" />
    <!-- Product Pagination attributes ends -->
    <!-- Product Pagination attributes -->
    <aura:attribute type="List" name="productsMasterList" />
    <aura:attribute name="productSortBy" type="String" access="PRIVATE" default="purchasedOn|DESC" />
    <aura:attribute name="productPagination" type="Map" />
    <aura:attribute name="currentProductPage" type="Integer" default="1" />
    <!-- Product Pagination attributes ends -->
    <!-- Orders Pagination attributes -->
    <aura:attribute type="List" name="ordersMasterList" />
    <aura:attribute name="ordersSortBy" type="String" access="PRIVATE" default="orderDate|DESC" />
    <aura:attribute name="ordersPagination" type="Map" />
    <aura:attribute name="currentOrdersPage" type="Integer" default="1" />
    <!-- Product Pagination attributes ends -->
    <aura:attribute name="totalProducts" type="String" access="PRIVATE" />
    <aura:attribute name="productsCount" type="String" />
    <aura:attribute name="totalOrderCount" type="String" />
    <aura:attribute name="totalQuickListCount" type="String" />
    <aura:attribute name="totalDocumentCount" type="String" />
    <aura:attribute name="ordersSelected" type="Boolean" default="false" />
    <aura:attribute name="quicklistSelected" type="Boolean" default="false" />
    <aura:attribute name="sdsSelected" type="Boolean" default="false" />
    <!--  <aura:handler event="c:EC_searchText" action="{!c.handleApplicationEvent}"/> -->
    <aura:attribute name="searchedStringValue" type="String" />

    <aura:handler name="init" value="{!this}" action="{!c.init}" />
    <!--<aura:handler name="setAttribute" event="c:setAttributeEvent" action="{!c.setAttributeValue}"/>
    -->
     <aura:attribute name="showSpinner" type="Boolean" default="true"/>

   <aura:if isTrue="{!v.showSpinner}" >
        <div class="slds-spinner_container">
        <div role="status" class="slds-spinner slds-spinner--medium">
            <span class="slds-assistive-text">Loading</span>
            <div class="slds-spinner__dot-a"></div>
            <div class="slds-spinner__dot-b"></div>
        </div>
        </div>
    </aura:if>
    <aura:if isTrue="{!!v.showSpinner}" >
    <!-- Dummy Tabs -->
