    </recordTypes>
    <validationRules>
        <fullName>Closed_case_cannot_be_reopened</fullName>
        <active>true</active>
        <description>To restrict user, closed case can&apos;t be opened again.</description>
        <errorConditionFormula>IF( RecordType.Name == &apos;Support&apos; &amp;&amp;  ISPICKVAL(PRIORVALUE(Status),&apos;Closed&apos;) &amp;&amp;  ISCHANGED( Status), true, false)</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>Closed case can&apos;t be reopen.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Community_User_Case_Status_Validate</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
				 RecordType.Name = &apos;Support&apos;,
					NOT(ISNEW()), ISCHANGED(Status),  
					OR($Profile.Name=&apos;Sales Rep&apos;, $Profile.Name =&apos;Ecolab Customer Community Plus Users&apos;),
					TEXT(Status) != &apos;Closed&apos;,
				 OR(TEXT(Status) != &apos;In Progress&apos;, 
     TEXT(PRIORVALUE(Status)) != &apos;Awaiting Customer Response&apos;)
)</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>You cannot change case status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Res_Demandatory_for_resolved_case</fullName>
        <active>true</active>
        <description>Restrict user to save case record without putting Resolution Description. If case is resolved then Description is mandatory.</description>
        <errorConditionFormula>AND( RecordType.DeveloperName = &apos;Support&apos;,
				ISCHANGED(Status),
				ISPICKVAL(Status, &apos;Resolved&apos;),
				OR(ISBLANK(Resolution_Description__c), ISBLANK( TEXT(Resolution_Category__c)))
)</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>Resolution Description and Resolution Category are mandatory for resolved case.</errorMessage>
    </validationRules>
</CustomObject>
