<aura:component implements="forceCommunity:availableForAllPageTypes" access="global" >
    <aura:attribute name="isarticleactive" type="boolean" default="true"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <div class="slds-tabs_default ec_community_support_tabs">

        <ul class="slds-tabs_default__nav defaulynavcls">
            <li class="{!if(v.isarticleactive,'slds-tabs_default__item slds-is-active textcls', 'slds-tabs_default__item textcls')}" title="Articles">
                <a class="slds-tabs_default__link textcls" href="/s/support"  tabindex="0"  id="tab-default-1__item">Articles</a>
            </li>
            <li class="{!if(v.isarticleactive,'slds-tabs_default__item textcls', 'slds-tabs_default__item slds-is-active textcls')}" title="Case List">
                <a class="slds-tabs_default__link textcls" href="/s/caselist"  tabindex="1"  id="tab-default-2__item">Case List</a>
            </li>
        </ul>
    </div>
    
    <c:Support_BreadCrumb/>
    <c:Support_Styles/>
    
</aura:component>