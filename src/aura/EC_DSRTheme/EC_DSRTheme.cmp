<aura:component implements="forceCommunity:themeLayout" access="global" description="Digital Service Report Theme">
    <!-- Importing the branding theme in the page -->
    <ltng:require styles="{!$Resource.EC_Branding_Theme  + '/css/theme.css'}" />
    <!-- Importing the jQuery  -->
    <ltng:require scripts="{!$Resource.EC_Branding_Theme  + '/js/jquery-3.3.1.min.js'}" afterScriptsLoaded="{!c.doInit}" />
    <aura:attribute name="header" type="Aura.Component[]" required="false"/>
    <aura:attribute name="footer" type="Aura.Component[]" required="false"/>
    <div class="header">{!v.header}</div>
    <div class="content">{!v.body}</div>
    <footer>{!v.footer}</footer>
</aura:component>