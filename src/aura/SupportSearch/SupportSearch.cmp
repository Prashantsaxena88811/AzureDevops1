<aura:component implements="forceCommunity:availableForAllPageTypes">
    <aura:attribute name="searchText" type="String" default=""/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <br/><br/>
    <div class="slds-grid">
        <div class="slds-col slds-size_4-of-6">
            <div class="slds-form-element slds-lookup" data-select="single">
                <div class="slds-form-element__control">
                    <div class="slds-input-has-icon slds-input-has-icon--right" onkeyup="{! c.handleKeyUp }">
                        
                        <lightning:buttonIcon iconName="utility:search" variant="bare" onclick="{! c.handleClick }"  alternativeText="Search" class="slds-input__icon" />
                        <ui:inputText value="{!v.searchText}" class="slds-lookup__search-input slds-input" placeholder="Search Articles" />
                    </div>
                </div>
            </div>
        </div>
        <div class="slds-col slds-size_1-of-6"></div>
        <div class="slds-col slds-size_1-of-6"></div>
    </div>
    <br/>
</aura:component>